{
  "name": "nuber-server",
  "version": "1.0.0",
  "description": "Server for Cloning Uber. GraphQL, Typescript, NodeJS",
  "main": "index.js",
  "repository": "https://github.com/shkimroy/nuber-server.git",
  "author": "shkim <shkim.roy.d@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.0.7",
    "gql-merge": "^0.0.6",
    "graphql-to-typescript": "^0.1.2",
    "nodemon": "^1.19.1",
    "ts-node": "^8.2.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "scripts": {
    "dev": "cd src && nodemon --exec ts-node index.ts -e ts,graphql",
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/types/graph.d.ts"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "helmet": "^3.18.0",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "typeorm": "^0.2.18"
  }
}
